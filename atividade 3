programa
{
	real numero1, numero2, numero3
	
	funcao inicio()
	{
		escreva("Digite um valor: ")
		leia(numero1)

		escreva("Digite um valor (diferente do anterior): ")
		leia(numero2)

		escreva("Digite um valor (diferente dos anteriores): ")
		leia(numero3)

		se (numero1 > numero2 e numero1 > numero3)
		{
			escreva("O maior número digitado foi o :", numero1)
		}

		senao se (numero2 > numero1 e numero2 > numero3)
		{
			escreva("O maior número digitado foi o :", numero2)
		}

		senao
		{
			escreva("O maior número digitado foi o : ", numero3)
		}
	}
}
